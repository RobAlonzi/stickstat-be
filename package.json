{
  "name": "stickstat-be",
  "version": "0.0.0",
  "description": "Backend for Stickstat",
  "devDependencies": {
    "@types/jest": "~27.0.1",
    "@types/node": "~14.14.45",
    "@typescript-eslint/eslint-plugin": "~4.31.1",
    "@typescript-eslint/parser": "~4.31.1",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-jest": "~24.4.2",
    "jest": "~27.2.0",
    "prettier": "~2.4.1",
    "rimraf": "~3.0.2",
    "ts-jest": "~27.0.5",
    "tsc-alias": "^1.4.0",
    "typescript": "~4.4.3"
  },
  "scripts": {
    "start": "node -r tsconfig-paths/register build/main.js",
    "build": "tsc -p tsconfig.release.json && tscpaths -p tsconfig.release.json",
    "clean": "rimraf coverage build tmp",
    "prebuild": "npm run lint",
    "build:watch": "tsc -w -p tsconfig.release.json",
    "lint": "eslint . --ext .ts,.tsx",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "watch": "tsc -w -p tsconfig.json"
  },
  "author": "Rob Alonzi <info@robalonzi.com>",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.22.0",
    "mysql": "^2.18.1"
  }
}
